<script>

    function exportCSV() {

        var fileName = 'LiniaWS7_';

        if ($('#Etap1').is(':checked') && $('#idE1').find('tr').length > 0)
            $('#idE1').table2csv({
                filename: fileName + 'Etap1.csv',
                separator: ';'
            });

        if ($('#Etap2').is(':checked') && $('#idE2').find('tr').length > 0)
            $('#idE2').table2csv({
                filename: fileName + 'Etap2.csv',
                separator: ';'
